{
  "customer-lobby": {
    "login": {
      "url": "http://localhost:3000/sso_login",
      "method": "POST",
      "tokenIdentifier": "cl"
    },
    "passwordReset": {
      "url": "http://localhost:3000/graphql",
      "type": "graphql",
      "query": "mutation ResetPassword($email: String!, $type: String) {\n  resetPassword(email: $email, type: $type) {\n    emailNotFound\n    success\n    __typename\n  }\n}\n",
      "params": {
        "type": "company"
      },
      "variableMap": {
        "email": "email"
      }
    },
    "sessionExchange": {
      "url": "http://localhost:3004/auth/_cb/everpro"
    }
  }
}